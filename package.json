{
  "name": "lazy-pix",
  "type": "module",
  "version": "1.0.0-beta.3",
  "private": false,
  "packageManager": "pnpm@7.22.0",
  "description": "Simple Vue and Nuxt plugin to lazy-load images",
  "author": "Moses Laurence<me.mlaure@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ijkml/lazy-pix#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ijkml/lazy-pix.git"
  },
  "bugs": "https://github.com/ijkml/lazy-pix/issues",
  "keywords": [
    "lazy-pic",
    "vue-lazy-image",
    "vue-lazy",
    "nuxt-image",
    "vue-image",
    "lazy-bg",
    "lazy-image",
    "lazy-img"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lintf": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "release": "bumpp && npm publish --access=public",
    "release:beta": "bumpp && npm publish --tag beta --access=public"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.31.0",
    "pnpm": "^7.22.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.22"
  }
}
